    "version": "0.2.0",
    "configurations": [
        {
            "type": "ziher-dbg",
            "request": "launch",
            "name": "Launch hello_world_proj",
            "program": "${workspaceRoot}/../bazel-bin/hello_world_proj/hello_proj_static.exe",
            "workingDir": "${workspaceRoot}/",
            "sourcePaths": [
                "${workspaceRoot}",
                "${workspaceRoot}/common",
                "${workspaceRoot}/threads"
            ],
            "preLaunchTask": "compileDebug",
            "terminalType": "integrated"
        }
    ]
}